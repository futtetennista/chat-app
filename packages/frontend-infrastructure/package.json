{
  "name": "@chat-app/frontend-infrastructure",
  "version": "1.0.0",
  "packageManager": "pnpm@9.12.2",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist && rm tsconfig.tsbuildinfo",
    "compile": "tsc -b",
    "c": "pnpm run clean && pnpm run compile",
    "generate-bindings": "cdktf get",
    "synth": "cdktf synth",
    "synth:clean": "rm -rf ./cdktf.out",
    "synth:local": "FRONTEND_INFRASTRUCTURE_JSON_CONFIG=$(cat config.json | jq -r -c .) pnpm run synth:clean && cdktf synth",
    "test:unit": "jest --passWithNoTests"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "fp-ts": "catalog:",
    "io-ts": "catalog:"
  },
  "devDependencies": {
    "@cdktf/provider-aws": "^19.42.0",
    "@jest/globals": "catalog:jest",
    "@swc/core": "^1.9.2",
    "@swc/jest": "catalog:jest",
    "@tsconfig/node20": "catalog:typescript-node20",
    "@types/jest": "catalog:jest",
    "@types/node": "catalog:typescript-node20",
    "cdktf": "^0.20.10",
    "constructs": "^10.4.2",
    "jest": "catalog:jest",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
